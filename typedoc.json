{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/index.ts"
  ],
  "entryPointStrategy": "expand",
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/demo/**/*",
    "src/plugins/**/*",
    "src/vite-env.d.ts"
  ],
  "out": "docs/api",
  "name": "ASCIIGround",
  "includeVersion": true,
  "readme": "README.md",
  "theme": "default",
  "plugin": [
    "typedoc-plugin-missing-exports"
  ],
  "sort": [
    "source-order",
    "alphabetical"
  ],
  "categorizeByGroup": true,
  "categoryOrder": [
    "Main",
    "Patterns", 
    "Rendering",
    "Utilities",
    "*"
  ],
  "groupOrder": [
    "Classes",
    "Interfaces", 
    "Type Aliases",
    "Functions",
    "Variables"
  ],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "EnumMember",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable", 
    "Function",
    "Accessor",
    "Method",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature"
  ],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "@internal": false
  },
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "treatWarningsAsErrors": false,
  "treatValidationWarningsAsErrors": false,
  "skipErrorChecking": false,
  "logLevel": "Info",
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "excludeExternals": true,
  "includeHierarchySummary": true,
  "searchInComments": true,
  "searchInDocuments": true,
  "cleanOutputDir": true,
  "titleLink": "https://github.com/Eoic/asciiground",
  "navigationLinks": {
    "GitHub": "https://github.com/Eoic/asciiground",
    "NPM": "https://www.npmjs.com/package/asciiground",
    "Demo": "https://eoic.github.io/ASCIIGround/demo"
  },
  "sidebarLinks": {
    "GitHub": "https://github.com/Eoic/asciiground", 
    "Report an Issue": "https://github.com/Eoic/asciiground/issues/new"
  },
  "hideGenerator": false,
  "githubPages": true,
  "sourceLinkTemplate": "https://github.com/Eoic/asciiground/blob/{gitRevision}/{path}#L{line}",
  "gitRevision": "master",
  "gitRemote": "origin",
  "disableSources": false,
  "basePath": "./src",
  "excludeTags": [
    "@internal"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  }
}
